Subject: [PATCH 39/58] use '~' in auto-complete path

---
 rc/emacs-rc-auto-complete.el | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/rc/emacs-rc-auto-complete.el b/rc/emacs-rc-auto-complete.el
index 523fd66..50b7d9a 100644
--- a/rc/emacs-rc-auto-complete.el
+++ b/rc/emacs-rc-auto-complete.el
@@ -1,9 +1,9 @@
-(add-to-list 'load-path "/home/lutts/emacs/elisp/auto-complete-1.3.1")
+(add-to-list 'load-path "~/emacs/elisp/auto-complete-1.3.1")
 
 (require 'auto-complete-config)
 (ac-config-default)
 
-(add-to-list 'ac-dictionary-directories "/home/lutts/emacs/elisp/auto-complete-1.3.1/dict")
+(add-to-list 'ac-dictionary-directories "~/emacs/elisp/auto-complete-1.3.1/dict")
 
 ;(setq ac-auto-show-menu nil)
 ;; show 1 seconds later
